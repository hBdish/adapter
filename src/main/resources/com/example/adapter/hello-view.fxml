<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="384.0" prefWidth="513.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="com.example.adapter.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ListView fx:id="listItems" prefHeight="200.0" prefWidth="200.0" />
   <TextField fx:id="input" />
   <Button mnemonicParsing="false" onAction="#addValue" text="Добавить" />
    <Button onAction="#delete" text="Удалить" />
   <Button mnemonicParsing="false" onAction="#find" text="Найти" />
</VBox>
